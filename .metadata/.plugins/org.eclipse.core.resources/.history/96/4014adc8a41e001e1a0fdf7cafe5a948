package metier;

public class Compte {
	// attribut de la class
	private int code;
	private float solde;
	private static int nbComptes; 
	
	// constructor sans param√®tre
	public Compte() {
	}
	// constructor avec parametres
	public Compte(float s) {
		++nbComptes;
		code = nbComptes;
		solde = s;
	}
	
	//Methode de la class
	
	
	

}
