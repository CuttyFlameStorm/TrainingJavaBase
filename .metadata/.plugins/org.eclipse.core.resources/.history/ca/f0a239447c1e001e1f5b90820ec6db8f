package metier;

public class Compte {
	private int code;
	private float solde;
	
	//Constructor
	public Compte(int c, float s) {
		code = c;
		solde = s;
	}
	
	// MÃ©thode de la class Compte
	public void verser(float mt) {
		solde=solde+mt;
	}
	public void retirer(float mt) {
		solde=solde-mt;
	}

//	@Override
//	public String toString() {
//		return "Compte [code=" + code + ", solde=" + solde + "]";
//	}
	
	

}
